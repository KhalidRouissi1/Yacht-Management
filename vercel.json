{
    "version": 2,
    "builds": [
        {
            "src": "yachtManagment/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python3.9"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {"disDir": "static"}
        }
    ],
    "routes": [
        {"src": "/static/(.*)", "dest": "static/$1"},
        {"src": "/(.*)", "dest": "yachtManagment^/wsgi.py"}
    ]
}
